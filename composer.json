{
  "name": "valiant-bedrock/libforms",
  "description": "A small form library built for PocketMine-MP",
  "type": "library",
  "license": "MIT",
  "version": "0.8.0",
  "autoload": {
    "psr-4": {
      "libforms\\": "src/libforms"
    }
  },
  "scripts": {
    "analyze": "./vendor/bin/phpstan analyze",
    "run-cs-fixer": "./vendor/bin/php-cs-fixer fix --diff --ansi"
  },
  "authors": [
    {
      "name": "Matthew Jordan",
      "email": "matthew@valiantnetwork.xyz"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.2"
  },
  "require-dev": {
    "pocketmine/pocketmine-mp": "dev-legacy/pm4",
    "phpstan/phpstan": "1.10.15",
    "friendsofphp/php-cs-fixer": "v3.17.0"
  }
}